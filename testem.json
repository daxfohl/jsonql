{
    "framework": "mocha",
    "src_files": [
        "src/*.coffee",
        "test/*Tests.coffee"
    ],
    "launchers": {
        "all": {
          "command": "mocha -R tap --compilers coffee:coffee-script/register test/*Tests.coffee",
          "protocol": "tap"
        }
    },
   "launch_in_dev": [ "all" ],
   "launch_in_ci": [ "all" ]
}